#include <tunables/global>

/usr/sbin/ircd-hybrid flags=(complain) {
  #include <abstractions/apache2-common>
  #include <abstractions/base>

  /usr/sbin/ircd-hybrid mr,
  /usr/share/ircd-hybrid r,
  /usr/lib/ircd-hybrid/** mr,
  /var/log/ircd/** mrw,
  /run/ircd/* rw,
  /var/run/ircd/* rw,
  /etc/ircd-hybrid/* r,

}